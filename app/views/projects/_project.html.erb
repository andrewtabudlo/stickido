<div class="container">
<div id="project-<%= project.id %>">
<p>Project Name: <%= project.name %></p>
<span>
	<button class="btn btn-primary"> Edit <%= link_to '', edit_project_path(project), remote: true %>
	</button>
</span>
<button class="btn btn-primary">
	<span id='#project-<%= project.id %>'> Delete<%= link_to '', project_path(project), method: :delete, data: { confirm: "Are you sure you want to delete '#{project.name}'?" } %>
	</button>
	</span>

<button class="btn btn-primary">Task
	<span><a href="/projects/<%= project.id %>/tasks" data-turbolinks="false"> </a> 
	</span>
</button>

<button class="btn btn-primary">Sticki 
	<span><a href="/projects/<%= project.id %>/stickis" data-turbolinks="false"> </a>
	</span>

</div>
</div>
<hr>
